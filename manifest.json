{
    "manifest_version": 2,
    "name": "Cookie Cleaner",
    "description": "This extension will remove all cookies of current website",
    "version": "1.0",
    "browser_action": {
        "default_icon": {
                        "19":"images/icon_19.png",
                        "38":"images/icon_38.png"
                        },
        "default_popup": "popup.html"
    },
    "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*" ]
}
