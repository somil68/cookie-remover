{
    "manifest_version": 2,
    "name": "Cookie Remover",
    "description": "This extension will remove all cookies of current website",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*" ]
}
